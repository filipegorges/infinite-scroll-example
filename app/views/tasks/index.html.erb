<p id="notice"><%= notice %></p>

<h1>Tasks</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Author</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="tasks">
    <%= render @tasks %>
  </tbody>
</table>

<%= will_paginate @tasks %>

<%= link_to 'New Task', new_task_path %>
